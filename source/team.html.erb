<div id="team">
  <div class="team-content">
    <h1>This template is built with <i class="fa fa-heart"></i> by</h1>

    <ul class="list-inline">
      <% data.users.each do |user| %>
      <li>
        <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_nickname}", class: "img-circle" %>
        <p><%= user.name %></p>
        <i class="fa fa-github"></i><%= link_to user.github_nickname, "https://github.com/#{user.github_nickname}", target: "_blank", class: "github-name" %>
      </li>
      <% end %>
    </ul>
  </div>
</div>



